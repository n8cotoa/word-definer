<div class="panel panel-default">
  <div class="panel-heading">
    <h2 class="panel-title">Here is your entire dictionary</h2>
  </div>
  <div class="panel-body">
    <% @dictionary.each do |entry| %>
      <h4><%= entry.word %></h4>
      <ul>
        <% entry.definition.each do |definition| %>
          <li><%= definition %></li>
        <% end %>
      </ul>
    <% end %>

    <a href="/" class="btn btn-danger">Back To Home</a>
  </div>
</div>
